# FEATURE: CONFIG_FEATURE_TAR_CREATE
mkdir -p foo/{1,2,3}
mkdir -p foo/1/{10,11}
mkdir -p foo/1/10/{100,101,102}
timeout -k 0.5 0.5 ./tar_reduced cf foo.tar -C foo .
rm -rf foo/*
timeout -k 0.5 0.5 ./tar_reduced xf foo.tar -C foo ./1/10
find foo | sort >logfile.bb
rm -rf foo/*
timeout -k 0.5 0.5 ./tar_reduced xf foo.tar -C foo ./1/10
find foo | sort >logfile.gnu
diff -u logfile.gnu logfile.bb
