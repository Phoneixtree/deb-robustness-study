# FEATURE: CONFIG_FEATURE_TAR_FROM
# FEATURE: CONFIG_FEATURE_TAR_CREATE
touch foo
touch bar
timeout -k 0.5 0.5 ./tar_reduced cf foo.tar foo bar
echo foo > foo.exclude
echo bar > bar.exclude
rm foo bar
timeout -k 0.5 0.5 ./tar_reduced xf foo.tar -X foo.exclude -X bar.exclude
test ! -f foo -a ! -f bar
